pipeline { 
    agent any 
         environment{
             APP = 'frontend'
             GIT_BRANCH = 'main'
              GIT_CREDENTIALS =  'git_dhanu'
              GIT_URL = 'https://github.com/dhanashreeraga/pipeline-first.git'
         }
         stages {
             stage('CHECKOUT') {
                 steps {
                    script  { 
                       

                  git branch: "${env.GIT_BRANCH}",
                      credentialsId: "${env.GIT_CREDENTIAL}",
                      url: "${env.GIT_URL}"
                 }
                 }
             }
             stage('CHECK'){
                steps {
                sh '''
                   ls -lrt
                   echo $GIT_BRANCH
                   env
                   '''
             }
             }
         }
     }

