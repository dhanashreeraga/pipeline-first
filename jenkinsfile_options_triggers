pipeline {
   agent any

    options{
        ansiColor('xterm')
        buildDiscarder(logRotator(numToKeepStr: '5')) 
         disableConcurrentBuilds(abortPrevious: true) 
          retry(3) 
          timeout(time: 5, unit: 'seconds')
          timestamps() 
    }
    stages {
         stage('stage1'){
             steps{
                  sh 'sleep 10'
                
             }
         }
             
                 stage('stage2'){
             steps{
                sh 'sleep 10'
                  
             }
             }
    }
}